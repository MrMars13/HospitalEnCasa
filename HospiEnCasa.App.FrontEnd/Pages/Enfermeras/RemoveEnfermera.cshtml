@page
@model HospiEnCasa.App.FrontEnd.Pages.RemoveEnfermeraModel
@{
}
<h1>Confirmación</h1>
<p>Desea eliminar al enfermera/o @Model.Enfermera.Nombre @Model.Enfermera.Apellidos</p>
<form method = "post" class="mt-3">
    <input hidden asp-for="Enfermera.Id" />
    <button type = "submit" class="btn btn-primary"> Si </button>
    <a asp-page="./ListEnfermeras" class="btn btn-primary"> No </a>
</form>
